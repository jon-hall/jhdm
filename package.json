{
  "name": "jonhalldotme",
  "version": "0.0.0",
  "description": "jonhall.me",
  "private": true,
  "main": "dist/server/index.js",
  "scripts": {
    "build": "concurrent \"npm run build:webpack\" \"npm run build:server\" \"npm run build:jade\"",
    "build:server": "babel src/server -d dist/server/ --source-maps",
    "build:webpack": "SET NODE_ENV=production&& webpack --progress --profile --colors",
    "build:jade": "SET NODE_ENV=production&& jade src/client -P -o dist/client",
    "dev": "concurrent \"npm run dev:webpack\" \"npm run dev:server\" \"npm run dev:jade\" \"npm run opn\"",
    "opn": "opn http://localhost:3000",
    "dev:webpack": "webpack-dev-server --progress --profile --colors --hot",
    "babel-node": "babel-node --presets stage-0",
    "dev:server": "nodemon --exec npm run babel-node -- src/server/index.es6",
    "dev:jade": "jade src/client -w -P -o dist/client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jon-hall/jonhalldotme"
  },
  "keywords": [],
  "author": "Jon Hall <jon_hall@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jon-hall/jonhalldotme/issues"
  },
  "homepage": "https://github.com/jon-hall/jonhalldotme#readme",
  "dependencies": {
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-static": "^1.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "6.3.26",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "concurrently": "^1.0.0",
    "css-loader": "0.23.1",
    "file-loader": "0.8.4",
    "jade-cli": "^0.1.1",
    "json-loader": "^0.5.4",
    "nib": "^1.1.0",
    "node-libs-browser": "0.5.3",
    "nodemon": "^1.8.1",
    "opn-cli": "^1.0.0",
    "react": "0.14.5",
    "react-dom": "0.14.3",
    "react-hot-loader": "1.2.7",
    "style-loader": "0.13.0",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  }
}
